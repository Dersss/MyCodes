
WHILE next-is-wall DO
  IF next-is-not-wall THEN
    WHILE next-is-enemy DO
      IF next-is-not-enemy THEN
        infect
        move
        turnleft
        skip
        turnright
      ELSE
        infect
        move
        turnleft
        skip
        turnright
      END IF
    END WHILE
  END IF
END WHILE
